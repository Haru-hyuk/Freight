{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "noEmit": true,
    "resolveJsonModule": true,
    "lib": ["ESNext", "DOM"],
    "jsx": "react-native",
    "moduleResolution": "Bundler",
    "target": "ESNext",
    "esModuleInterop": true,
    "allowJs": true,
    "module": "ESNext",
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "types": ["expo-router/types"]
  }
}
